{
  "Kestrel": {
    "Limits": {
      "MaxConcurrentConnections": 500,
      "MaxConcurrentUpgradedConnections": 600
    },
    "DisableStringReuse": true
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information"
    }
  },
  "AllowedHosts": "*",
  "Startup": {
    "General": {
      "ServiceName": "Test.Store.Fruit",
      "ServiceUri": "/api/v1/TestStoreFruit",
      "Scopes": "platform",
      "LogCategory": "Test.Store.Fruit-svc"
    },
    "Discovery": {
      "RegisterEndpoint": false
    }
  }
}
